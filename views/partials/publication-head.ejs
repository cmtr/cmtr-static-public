<div class="row">
	<h2><%= publication.title %></h2>
	<p><strong><%= publication.subTitle %></strong></p>
	<div>
		<% if (publication.images.head.urls) { %>
			<img 
				src="<%= publication.images.head.urls.extra %>" 
				srcset= ""
			<#%	sizes=""
				alt="<%= publication.images.head.alt %>"  
				class="img-fluid" 
			/>
		<% } else { %>
			<img 
				src="<%= publication.images.head.src %>" 
				alt="<%= publication.images.head.alt %>"  
				class="img-fluid" 
			/>
		<% } %>
		<p><strong>Image text:</strong> <%= publication.images.head.paragraph %></p>
	</div>
</div>